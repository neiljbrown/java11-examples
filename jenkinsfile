pipeline {
    agent {label 'manam-1'}
    triggers { upstream(upstreamProjects: 'dum', threshold: hudson.model.Result.SUCCESS) }
    stages {
        stage ('scm') {
            steps {
                git 'https://github.com/yadavallimallikharjua/java11-examples.git'
            }
        }
        stage ('build') {
            steps {
                sh 'mvn clean package'
            }
        }
    }  
}
